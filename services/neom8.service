[Unit]
Description=NEOM8 GPS config binary
Before=adxl355.service gpsd.service

[Service]
Type=notify
EnvironmentFile=/home/pi/SEISREC/parameter
User=root
ExecStart=$repo_root/SEISREC/unit/unit_neom8 -f $gnss_freq -u -c
CPUSchedulingPolicy=fifo
CPUSchedulingPriority=1


[Install]
WantedBy=multi-user.target
