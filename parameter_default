adxl_mode=int[0,1]
adxl_sps=float[4000.0,2000.0,1000.0,500.0,250.0,125.0,62.5,31.25,15.625,7.8125,3.90625]
adxl_syncopt=int[0,1,2]
db_pass=string
db_port=int[0-9999]
