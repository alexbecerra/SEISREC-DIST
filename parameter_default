[Parameters]
sta_name=DEV00
sta_net=EEW
sys_acc_core=2
acc_range=2
acc_db_pass=$$earlywarningCSN
adxl_extclk=0
adxl_mode=0
adxl_sps=125
adxl_syncopt=2
gnss_freq=100
rtc_snooze_h=0
rtc_snooze_m=6
rtc_snooze_s=0
dyn_host=null.dyndns.org
dyn_pass=password
dyn_user=user
file_db_host=127.0.0.1
file_db_name=LOCALBUF
file_db_pass='$$earlywarningCSN'
file_db_port=6379
file_pipeline=500
file_max_qty=5
file_path=/home/pi/
repo_root=/home/pi

[DESCRIPTIONS]

sta_name=Nombre de la estacion (Por defecto: DEV00)
sta_net=Nombre de la red a la cual pertenece la estacion (Por defecto: EEW)
sys_acc_core=Nucleo del procesador en el cual corre el proceso ADXL355 [0-3] (Por defecto: 2)
acc_range=Rango del acelerometro ADXL355 en g [2, 4, 8] (Por defecto: 2)
acc_db_pass=Contraseña para la base de datos en la rutina del acelerometro (Por defecto: $$earlywarningCSN)
adxl_extclk=Habilitar/Deshabilitar el reloj externo para el acelerometro ADXL355 [0 = desabilitado, 1 = habilitado] (Por defecto: 0)
adxl_mode=Modo de toma de datos para el acelerometro ADXL355 [0 = Desde la memoria FIFO, 1 = Por interrupcion] (Por defecto: 0)
adxl_sps=Tasa de muestreo interna, en Hz, del acelerometro ADXL355 [4000.0, 2000.0, 1000.0, 500.0, 250.0, 125.0, 62.5, 31.25, 15.625, 7.8125, 3.90625] (Por defecto: 125)
adxl_syncopt=ADXL355 Sync option [0, 1, 2] (Por defecto: 2)
gnss_freq=Frecuencia de la señal adicional del dispositivo GNSS en Hz (Por defecto: 100)
rtc_snooze_h=Horas para el aplazamiento de la alarma [0 - 23] (Por defecto: 0)
rtc_snooze_m=Minutos para el aplazamiento de la alarma [0 - 59] (Por defecto: 6)
rtc_snooze_s=Segundos para el aplazamiento de la alarma [0 - 59] (Por defecto: 0)
dyn_host=Nombre de host para el DNS dinamico (Por defecto: null.dyndns.org)
dyn_pass=Contraseña para la cuenta de DNS dinamico (Por defecto: password)
dyn_user=Nombre de usuario para la cuenta de DNS dinamico (Por defecto: user)
file_db_host=Direccion IP de la base de datos local (Por defecto: 127.0.0.1)
file_db_name=Nombre de la base de datos local para transferencia a archivos (Por defecto: LOCALBUF)
file_db_pass=contraseña de la base de datos local (Por defecto: '$$earlywarningCSN')
file_db_port=Puerto TCP de la base de datos local (Por defecto: 6379)
file_pipeline=Numero de lineas a transferir desde una base de datos a un archivo en una llamada a la base de datos (Por defecto: 500)
file_max_qty=Numero maximo de archivos a mantener en el disco (Por defecto: 5)
file_path=Directorio en el cual se guardan los archivos (Por defecto: /home/pi/)
repo_root=Directorio donde está instalado la carpeta SEISREC