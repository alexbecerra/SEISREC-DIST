[PARAMETERS]
sta_name=DEV00
sta_net=C2
sys_acc_core=2
acc_range=2
adxl_extclk=0
adxl_mode=0
adxl_sps=125
adxl_syncopt=2
db_pass=earlywarningCSN_UChile
db_port=6379
gnss_freq=100
gnss_if=uart
rtc_snooze_h=0
rtc_snooze_m=15
rtc_if=local
dyn_host=null.dyndns.org
dyn_int=noint
dyn_pass=password
dyn_time=5
dyn_user=user
file_db_host=127.0.0.1
file_db_name=LOCALBUF
file_pipeline=500
file_max_qty=4
file_path=/home/pi/

[DESCRIPTIONS]
sta_name=Nombre de la estación
sta_net=Nombre de la red a la cual pertenece la estación
sys_acc_core=Nucleo del procesador en el cual corre el proceso ADXL355
acc_range=Rango del acelerómetro ADXL355 en g
adxl_extclk=Habilitar/Deshabilitar el reloj externo para el acelerómetro ADXL355 [0 = desabilitado, 1 = habilitado]
adxl_mode=Modo de toma de datos para el acelerómetro ADXL355 [0 = Desde la memoria FIFO, 1 = Por interrupción]
adxl_sps=Tasa de muestreo interna, en Hz, del acelerómetro ADXL355
adxl_syncopt=Opción de sincronización del integrado ADXL355
db_pass=Contraseña para la base de datos del sistema
db_port=Puerto TCP de la base de datos del sistema
gnss_freq=Frecuencia, en Hz, de la señal de sincronización proveniente del dispositivo GNSS
gnss_if=Interfaz de comunicación con el sistema GNSS
rtc_snooze_h=Horas para el aplazamiento de la alarma
rtc_snooze_m=Minutos para el aplazamiento de la alarma
rtc_if=Fuente de tiempo para configurar la alarma [RTC (real time clock), Hora del sistema (local)]
dyn_host=Nombre de host para el DNS dinámico
dyn_int=Interfaz a usar para la conexión a Internet [ppp0, wlan0, eth0, noint (Deshabilita este programa)]
dyn_pass=Contraseña para la cuenta de DNS dinámico
dyn_time=Tiempo de espera en minutos para chequear cambios de IP y actualizar el DNS dinámico
dyn_user=Nombre de usuario para la cuenta de DNS dinámico
file_db_host=Dirección IP de la base de datos local
file_db_name=Nombre de la base de datos local para transferencia a archivos
file_pipeline=Número de líneas a transferir desde una base de datos a un archivo en una única llamada
file_max_qty=Número máximo de archivos a mantener en el disco
file_path=Directorio en el cual se guardan los archivos

